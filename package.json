    "commander": "^13.1.0",
    "eventsource": "^3.0.6",
    "jest": "^29.7.0",
    "nanospinner": "^1.2.2",
    "simple-git": "^3.27.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
