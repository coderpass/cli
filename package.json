{
  "name": "@coderpass/cli",
  "version": "1.0.18",
  "description": "Coderpass CLI",
  "main": "dist/coderpass.js",
  "type": "commonjs",
  "bin": {
    "coderpass": "./dist/coderpass.js"
  },
  "scripts": {
    "build": "tsc",
